{
  "name": "pn-blog-hub",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "npm run build:posts && npm run pages:build",
    "build:posts": "ts-node --project tsconfig.builder.json ./src/builder/posts.ts",
    "vercel-build": "next build",
    "pages:build": "npm run build:posts && npx @cloudflare/next-on-pages",
    "preview": "npm run build && wrangler pages dev",
    "deploy": "npm run build && wrangler pages deploy"
  },
  "dependencies": {
    "dayjs": "^1.11.0",
    "next": "^15.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^5.3.0"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.4",
    "@types/react-dom": "^18.0.0",
    "eslint": "^8.12.0",
    "eslint-config-next": "14.2.15",
    "eslint-plugin-next-on-pages": "^1.13.5",
    "fs-extra": "^11.2.0",
    "npm-run-all": "^4.1.5",
    "rss-parser": "^3.12.0",
    "sass": "^1.85.0",
    "ts-node": "^10.7.0",
    "typescript": "^5.8.3"
  },
  "license": "MIT"
}
